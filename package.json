    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-polyfill": "^6.26.0",
    "copy-webpack-plugin": "^5.0.2",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "expose-loader": "^0.7.5",
    "file-loader": "^3.0.1",
    "gh-pages": "^2.0.1",
    "glslify": "^7.0.0",
    "glslify-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "lodash.throttle": "^4.1.1",
    "mini-css-extract-plugin": "^0.5.0",
    "mobx": "^5.9.4",
    "mobx-react": "^5.4.3",
    "node-sass": "^4.11.0",
    "phaser": "^3.16.2",
    "prettier": "^1.17.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "sass-loader": "^7.1.0",
    "store": "^2.0.12",
    "style-loader": "^0.23.1",
    "ts-loader": "^5.4.4",
    "typescript": "^3.4.5",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.3.1"
  },
  "prettier": {
    "printWidth": 100
  },
  "scripts": {
    "update-types": "curl https://raw.githubusercontent.com/photonstorm/phaser3-docs/master/typescript/phaser.d.ts --create-dirs --location --output ./types/phaser.d.ts",
    "postinstall": "npm run update-types",
    "build": "webpack --mode production",
    "dev": "webpack-dev-server --mode development --open",
    "dev:production": "webpack-dev-server --mode production --open",
    "clean": "rm -rf ./public",
    "predeploy": "npm run clean && npm run build",
    "deploy": "gh-pages --dist public --dest .",
    "prettier": "prettier --write ./src/js/**/*.{js,jsx,json}"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sporadic-labs/phaser-3-sandbox.git"
  },
  "bugs": {
    "url": "https://github.com/sporadic-labs/phaser-3-sandbox.git/issues"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.3",
    "easystarjs": "^0.4.3",
    "phaser3-plugin-isometric": "0.0.7"
  }
}
